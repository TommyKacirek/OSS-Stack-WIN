:80 {
    # Wazuh Dashboard
    handle_path /wazuh* {
        reverse_proxy wazuh-dashboard:5601 {
            transport http {
                tls_insecure_skip_verify
            }
        }
    }

    # Greenbone GSA
    handle_path /greenbone* {
        reverse_proxy greenbone-docker-gsa-1:80
    }
    
    # Root redirect or generic landing page can be added here
}
